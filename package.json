    "config": "^3.3.6",
    "express": "^4.17.1",
    "mongoose": "^5.13.3",
    "normalize-css": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
