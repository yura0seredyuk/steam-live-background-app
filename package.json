{
  "name": "webpack_template",
  "version": "1.0.0",
  "description": "webpack_template",
  "private": "true",
  "scripts": {
    "dev": "concurrently \"npm run webpack-start-server\" \"npm run server-start-dev\"",
    "prod": "concurrently \"npm run server-start-prod\" \"npm run webpack-build-prod\"",
    "webpack-start-server": "webpack serve",
    "webpack-watch": "webpack --watch",
    "webpack-build-dev": "webpack",
    "webpack-build-prod": "NODE_ENV=production webpack",
    "server-start-dev": "nodemon server.js",
    "server-start-prod": "node server.js",
    "install-dependencies": "npm install",
    "stats": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "keywords": [],
  "author": "Yura Seredyuk <yura.seredyuk0@gmail.com>",
  "license": "ISC",
